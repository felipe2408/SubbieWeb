@{
    ViewBag.Title = "Registrer";
    Layout = "~/Views/Shared/_LayaoutLoginRegistrer.cshtml";
}
}

<link href="~/Content/Register.css" rel="stylesheet" />

<header>
    <img src="~/img/Trazado -1.png" />
    <img src="~/img/Trazado -2.png" />
</header>

<div class="contenedor_principal">
    <div class="form-container">
        <div class="titulo_inicia_sesion">
            Registrate
        </div>
        <form method="get" class=" contenedor_formulario">
            <input type="email" placeholder="nombre" name="" class="" required>
            <input type="password" placeholder="clave" class="" required>
            <input type="password" placeholder="Confirmar clave" class="" required>
            <input type="button" value="Crear cuenta" class="Ingresar botones-principales">
        </form>
        <div class="contenedor_iconos">
            <div class="iconos"></div>
            <div class="iconos"></div>
            <div class="iconos"></div>
        </div>
    </div>
</div>
<footer>
    <img src="~/img/Trazado -1.png" />
    <img src="~/img/Trazado -2.png" />
</footer>
<script src="~/Scripts/ValidacionRegistrerUsuario.js"></script>